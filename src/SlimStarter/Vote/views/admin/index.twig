{% extends 'admin/index.twig' %}
{% block content %}
<div class="row page-header-box">
    <div class="col-xs-10">
        <h3>{{ title }}</h3>
    </div>
    <div class="col-xs-2">
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="table-responsive">
            <table class="table table-striped table-condensed">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Entry Voted For</th>
                        <th>IP Address</th>
                    </tr>
                </thead>
                <tbody id="user-table">
                    {% for vote in votes.data %}
                    <tr id="user-row-{{ vote.id }}">
                        <td>{{ vote.id }}</td>
                        <td>{{ vote.entry.name }}</td>
                        <td>{{ vote.ip_address_formatted }}</td>
                    </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
</div>
{% endblock %}